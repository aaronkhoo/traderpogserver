<h1>Listing item_infos</h1>

<table>
  <tr>
    <th>Price</th>
    <th>Supplymax 1</th>
    <th>Supplyrate 1</th>
    <th>Supplymax 2</th>
    <th>Supplyrate 2</th>
    <th>Supplymax 3</th>
    <th>Supplyrate 3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @item_infos.each do |item_info| %>
  <tr>
    <td><%= item_info.price %></td>
    <td><%= item_info.supplymax_1 %></td>
    <td><%= item_info.supplyrate_1 %></td>
    <td><%= item_info.supplymax_2 %></td>
    <td><%= item_info.supplyrate_2 %></td>
    <td><%= item_info.supplymax_3 %></td>
    <td><%= item_info.supplyrate_3 %></td>
    <td><%= link_to 'Show', item_info %></td>
    <td><%= link_to 'Edit', edit_item_info_path(item_info) %></td>
    <td><%= link_to 'Destroy', item_info, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Item info', new_item_info_path %>
